function Map(props) {

  const onPathClick = (data) => {
    props.onPathClick(data)
  }

  const [selectedPathId, setSelectedPathId] = useState(props.data.id);
  const [tooltip, setTooltip] = useState({ visible: false, x: 257.8300018310547, y: 111.0570068359375, text: ' ' });
  const svgRef = useRef(null);

  const handlePathClick = (id, data, event) => {
    const svgElement = svgRef.current;
    if (svgElement) {
      const pathElement = svgElement.querySelector(`#${id}`);
      if (pathElement) {
        const bbox = pathElement.getBBox();
        console.log('BBox:', bbox);
        setTooltip({
          visible: true,
          x: bbox.x + bbox.width / 2, // Центрирование подсказки по X
          y: bbox.y - 10, // Положение подсказки над путем
          text: data.title
        });
        setSelectedPathId(id);
        onPathClick(data);
      }
    }
  };

  return (
    <div className={styles.mapBlock}>
      <div className={styles.mapBlockLeft}>
        <div className={styles.leftBtnblock}>
          <h1>Zebraliśmy dla Ciebie najlepszych specjalistów w całym kraju</h1>
          <button>Umów się na wizytę</button>
        </div>
        <div className={styles.staticBlock}>
          <div className={styles.staticItem}>
            <img src={img1} />
            <h3>{props.data.doctors}</h3>
            <p>profesjonalnych lekarzy</p>
          </div>
          <div className={styles.staticItem}>
            <img src={img2} />
            <h3>{props.data.specializations}</h3>
            <p>specjalizacji lekarskich</p>
          </div>
          <div className={styles.staticItem}>
            <img src={img3} />
            <h3>{props.data.locations}</h3>
            <p>miejscowości, w których umówisz się na wizytę</p>
          </div>
        </div>
      </div>
      <div className={styles.mapBlockRight}>
        <svg ref={svgRef} width="685" height="550" viewBox="0 0 685 550" fill="none" xmlns="http://www.w3.org/2000/svg">
          {mapPaths.map(({ id, pathData, data, title }) => (
            <g>
              <path
                key={id}
                id={id}
                d={pathData}
                fill={selectedPathId === id ? "#0E0E0E" : "#3E36B0"}
                onClick={(event) => handlePathClick(id, data, event)}
              />
              
            </g>

          ))}
          
        </svg>
      </div>

    </div>
  )
}
export default Map